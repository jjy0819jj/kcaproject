<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 클래스에 별명 설정 -->
	<typeAliases>
		<typeAlias type="statistic.bean.statisticDTO" alias="statisticDTO"/>
		<typeAlias type="statistic.bean.CommentDTO" alias="CommentDTO"/>
		<typeAlias type="admin.bean.coronaAvgDTO" alias="coronaAvgDTO"/>
	</typeAliases>
	

	<!-- SQL Mapper 설정 -->
	<mappers>
		<mapper resource="mybatis/main-mapping.xml"/>
		<mapper resource="mybatis/admin-mapping.xml"/>
		<mapper resource="mybatis/statistic-mapping.xml"/>
	</mappers>
	
</configuration>